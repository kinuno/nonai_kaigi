<div class="roomname-contents">
  <h2 class="room-comments-title"><%= @room.name %></h2>
</div>

<div class='character-contents'>
    <h2 class='characters-index'>Characters</h2>
    <ul class='characters-lists'>
        <% @characters.each do |character|%>
          <li class='character-list'>
            <%= link_to "#{character.name}", room_character_path(@room.id, character.id), class:"character-link" %> 
          </li>
        <% end %>
    </ul>
</div>